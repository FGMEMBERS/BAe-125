      <entry><ind>     0 </ind><dep>   0 </dep></entry>
      <entry><ind>  285 </ind>nd><dep>  27 </dep></entry>
      <entry><ind>  570 </ind><dep>  53 </dep></entry>
      <entry><ind>  855 </ind><dep>  78 </dep></entry>
      <entry><ind>  1140 </ind><dep> 105 </dep></entry>
